{
  "name": "@capire/shared-db",
  "version": "3.0.0",
  "description": "CAP Sample CDS model deployment for shared-db scenario",
  "dependencies": {
    "@capire/bookstore": "*",
    "@capire/orders": "*",
    "@capire/reviews": "*"
  },
  "devDependencies": {
    "@sap/cds": "^9",
    "@sap/cds-dk": "^9"
  },
  "cds": {
    "requires": {
      "db-ext": {
        "[development]": {
          "model": "@capire/bookstore/db/h2"
        },
        "[production]": {
          "model": "@capire/bookstore/db/hana"
        }
      }
    }
  }
}
